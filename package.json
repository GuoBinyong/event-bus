    "preview": "vite preview",
    "doc":"api-extractor run -c config/api-extractor.jsonc; api-documenter markdown -i ./temp -o doc"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.15",
    "@microsoft/api-extractor": "^7.24.1",
    "package-tls": "^1.2.2",
