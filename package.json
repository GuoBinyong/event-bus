{
  "name": "@gby/event-bus",
  "version": "1.2.0",
  "description": "event-bus 是基于原生 EventTarget 类来开发的事件总线，无依赖，充分复用原生的能力增加了易用性。拥有极少的代码就实现了 EventBus 的核心功能",
  "main": "dist/event-bus.umd.js",
  "module": "dist/event-bus.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "homepage": "https://github.com/GuoBinyong/event-bus#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuoBinyong/event-bus"
  },
  "bugs": {
    "url": "https://github.com/GuoBinyong/event-bus/issues",
    "email": "guobinyong@qq.com"
  },
  "keywords": [
    "事件总线",
    "event bus",
    "event",
    "EventTarget"
  ],
  "author": {
    "name": "郭斌勇",
    "email": "guobinyong@qq.com"
  },
  "license": "MIT",
  "files": [
    "dist/[^.]*",
    "types/[^.]*",
    "docs/[^.]*",
    "doc/[^.]*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "bunch": "vite build --mode bunch",
    "preview": "vite preview",
    "api": "api-extractor run -c api-extractor.jsonc -l -v",
    "doc": "npm run api ; api-documenter markdown -i ./temp -o doc/api"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.15",
    "@microsoft/api-extractor": "^7.24.1",
    "build-tls": "^1.3.11",
    "dts-bundle-generator": "^6.12.0",
    "package-tls": "^1.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.9"
  }
}
